# Configurações do Banco de Dados
DB_SERVER=seu_servidor_sql
DB_NAME=sua_base_de_dados
DB_USER=seu_usuario
DB_PASSWORD=sua_senha
DB_DRIVER=msnodesqlv8
DB_ENCRYPT=true

# Configurações do Servidor
PORT=3000

# Configurações de Sessão
SESSION_SECRET=sua_chave_secreta_muito_segura

# Configurações de Agendamento
# Horário para verificação automática de status dos objetivos
# Formato: 'minuto hora dia mês dia_da_semana'
# Exemplos:
# '0 0 * * *' = meia-noite todos os dias (padrão)
# '0 9 * * *' = 9:00 da manhã todos os dias
# '30 14 * * *' = 14:30 todos os dias
# '0 0 * * 1' = meia-noite toda segunda-feira
# '*/5 * * * *' = a cada 5 minutos (para testes)
OBJETIVO_CHECK_TIME=0 0 * * *

# Configurações de Sincronização
SYNC_INTERVAL_MINUTES=60